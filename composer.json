{
    "name": "metrixinfo/nova-iframe",
    "description": "A Laravel Nova iFrame field.",
    "keywords": [
        "laravel",
        "nova",
        "iframe",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Metrixinfo\\Nova\\Fields\\Iframe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metrixinfo\\Nova\\Fields\\Iframe\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
